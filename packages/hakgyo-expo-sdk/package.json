{
  "name": "hakgyo-expo-sdk",
  "version": "1.0.9",
  "description": "SDK for integrating hakgyo webapp authentication and API in Expo apps",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "lint": "eslint src",
    "test": "jest"
  },
  "keywords": [
    "expo",
    "react-native",
    "authentication",
    "betterauth",
    "hakgyo"
  ],
  "author": "",
  "license": "MIT",
  "peerDependencies": {
    "expo": ">=50.0.0",
    "react": ">=18.0.0",
    "react-native": ">=0.73.0",
    "expo-secure-store": ">=12.0.0"
  },
  "dependencies": {
    "better-auth": "^1.4.17",
    "@better-auth/expo": "^1.1.0"
  },
  "devDependencies": {
    "@types/react": "^18.2.0",
    "@types/react-native": "^0.72.0",
    "typescript": "^5.3.0",
    "tsup": "^8.0.0",
    "eslint": "^8.56.0"
  }
}
